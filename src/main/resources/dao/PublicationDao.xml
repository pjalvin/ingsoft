<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bo.ucb.edu.ingsoft.dao.PublicationDao">
    <insert id="create" parameterType="bo.ucb.edu.ingsoft.model.Publication">
        INSERT into Publication VALUES
        (null,#{idSeller},#{idSeller},#{idBrand},#{idColor},#{idCity},#{model},
        #{doorNumber},#{description},#{motor},#{price},1,#{transaction.txDate},
        #{transaction.txIduser},#{transaction.txHost},#{transaction.txUpdate})
    </insert>
    <select id="findByContactId" resultType="bo.ucb.edu.ingsoft.model.Publication">
        SELECT * FROM Publication LIMIT 10 OFFSET 15
    </select>
</mapper>
